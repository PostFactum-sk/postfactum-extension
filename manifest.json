{
  "manifest_version": 3,
  "name": "Post Factum",
  "short_name": "PostFactum",
  "version": "1.0",
  "description": "A framework for classifying trust for online sources",
  "icons": {
    "16": "icons/favicon-16x16.png",
    "32": "icons/favicon-32x32.png"
  },
  "action": {
    "default_icon": "icons/favicon-32x32.png",
    "default_popup": "dialog.html"
  },
  "permissions": [
    "tabs",
    "contextMenus"
  ]
}